
RUN PGM=NETWORK

  PAR NODES=10000000
  NETI[1] = %NETI_FILE1%.net
  NETI[2] = %NETI_FILE2%.net
  NETO    = COMPARE.net

  PHASE=INPUT
    _TOTALMISMATCH=0
  ENDPHASE

  PHASE=LINKMERGE
    MISMATCH = 0
    IF (LI.1.TOLLBOOTH  <> LI.2.TOLLBOOTH ) MISMATCH = MISMATCH + 1

    IF (LI.1.TOLLEA_DA  <> LI.2.TOLLEA_DA ) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLEA_S2  <> LI.2.TOLLEA_S2 ) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLEA_S3  <> LI.2.TOLLEA_S3 ) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLEA_VSM <> LI.2.TOLLEA_VSM) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLEA_SML <> LI.2.TOLLEA_SML) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLEA_MED <> LI.2.TOLLEA_MED) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLEA_LRG <> LI.2.TOLLEA_LRG) MISMATCH = MISMATCH + 1

    IF (LI.1.TOLLAM_DA  <> LI.2.TOLLAM_DA ) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLAM_S2  <> LI.2.TOLLAM_S2 ) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLAM_S3  <> LI.2.TOLLAM_S3 ) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLAM_VSM <> LI.2.TOLLAM_VSM) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLAM_SML <> LI.2.TOLLAM_SML) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLAM_MED <> LI.2.TOLLAM_MED) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLAM_LRG <> LI.2.TOLLAM_LRG) MISMATCH = MISMATCH + 1

    IF (LI.1.TOLLMD_DA  <> LI.2.TOLLMD_DA ) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLMD_S2  <> LI.2.TOLLMD_S2 ) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLMD_S3  <> LI.2.TOLLMD_S3 ) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLMD_VSM <> LI.2.TOLLMD_VSM) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLMD_SML <> LI.2.TOLLMD_SML) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLMD_MED <> LI.2.TOLLMD_MED) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLMD_LRG <> LI.2.TOLLMD_LRG) MISMATCH = MISMATCH + 1

    IF (LI.1.TOLLPM_DA  <> LI.2.TOLLPM_DA ) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLPM_S2  <> LI.2.TOLLPM_S2 ) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLPM_S3  <> LI.2.TOLLPM_S3 ) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLPM_VSM <> LI.2.TOLLPM_VSM) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLPM_SML <> LI.2.TOLLPM_SML) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLPM_MED <> LI.2.TOLLPM_MED) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLPM_LRG <> LI.2.TOLLPM_LRG) MISMATCH = MISMATCH + 1

    IF (LI.1.TOLLEV_DA  <> LI.2.TOLLEV_DA ) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLEV_S2  <> LI.2.TOLLEV_S2 ) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLEV_S3  <> LI.2.TOLLEV_S3 ) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLEV_VSM <> LI.2.TOLLEV_VSM) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLEV_SML <> LI.2.TOLLEV_SML) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLEV_MED <> LI.2.TOLLEV_MED) MISMATCH = MISMATCH + 1
    IF (LI.1.TOLLEV_LRG <> LI.2.TOLLEV_LRG) MISMATCH = MISMATCH + 1

    IF (MISMATCH > 0)
        _TOTALMISMATCH = _TOTALMISMATCH + 1
        print LIST="MISMATCH: ",MISMATCH,"; A=",A," B=",B
        print LIST="TOLLBOOTH : ",LI.1.TOLLBOOTH ," ", LI.2.TOLLBOOTH
        print LIST="TOLLEA_DA : ",LI.1.TOLLEA_DA ," ", LI.2.TOLLEA_DA 
        print LIST="TOLLEA_S2 : ",LI.1.TOLLEA_S2 ," ", LI.2.TOLLEA_S2 
        print LIST="TOLLEA_S3 : ",LI.1.TOLLEA_S3 ," ", LI.2.TOLLEA_S3 
        print LIST="TOLLEA_VSM: ",LI.1.TOLLEA_VSM," ", LI.2.TOLLEA_VSM
        print LIST="TOLLEA_SML: ",LI.1.TOLLEA_SML," ", LI.2.TOLLEA_SML
        print LIST="TOLLEA_MED: ",LI.1.TOLLEA_MED," ", LI.2.TOLLEA_MED
        print LIST="TOLLEA_LRG: ",LI.1.TOLLEA_LRG," ", LI.2.TOLLEA_LRG

        print LIST="TOLLAM_DA : ",LI.1.TOLLAM_DA ," ", LI.2.TOLLAM_DA 
        print LIST="TOLLAM_S2 : ",LI.1.TOLLAM_S2 ," ", LI.2.TOLLAM_S2 
        print LIST="TOLLAM_S3 : ",LI.1.TOLLAM_S3 ," ", LI.2.TOLLAM_S3 
        print LIST="TOLLAM_VSM: ",LI.1.TOLLAM_VSM," ", LI.2.TOLLAM_VSM
        print LIST="TOLLAM_SML: ",LI.1.TOLLAM_SML," ", LI.2.TOLLAM_SML
        print LIST="TOLLAM_MED: ",LI.1.TOLLAM_MED," ", LI.2.TOLLAM_MED
        print LIST="TOLLAM_LRG: ",LI.1.TOLLAM_LRG," ", LI.2.TOLLAM_LRG

        print LIST="TOLLMD_DA : ",LI.1.TOLLMD_DA ," ", LI.2.TOLLMD_DA 
        print LIST="TOLLMD_S2 : ",LI.1.TOLLMD_S2 ," ", LI.2.TOLLMD_S2 
        print LIST="TOLLMD_S3 : ",LI.1.TOLLMD_S3 ," ", LI.2.TOLLMD_S3 
        print LIST="TOLLMD_VSM: ",LI.1.TOLLMD_VSM," ", LI.2.TOLLMD_VSM
        print LIST="TOLLMD_SML: ",LI.1.TOLLMD_SML," ", LI.2.TOLLMD_SML
        print LIST="TOLLMD_MED: ",LI.1.TOLLMD_MED," ", LI.2.TOLLMD_MED
        print LIST="TOLLMD_LRG: ",LI.1.TOLLMD_LRG," ", LI.2.TOLLMD_LRG

        print LIST="TOLLPM_DA : ",LI.1.TOLLPM_DA ," ", LI.2.TOLLPM_DA 
        print LIST="TOLLPM_S2 : ",LI.1.TOLLPM_S2 ," ", LI.2.TOLLPM_S2 
        print LIST="TOLLPM_S3 : ",LI.1.TOLLPM_S3 ," ", LI.2.TOLLPM_S3 
        print LIST="TOLLPM_VSM: ",LI.1.TOLLPM_VSM," ", LI.2.TOLLPM_VSM
        print LIST="TOLLPM_SML: ",LI.1.TOLLPM_SML," ", LI.2.TOLLPM_SML
        print LIST="TOLLPM_MED: ",LI.1.TOLLPM_MED," ", LI.2.TOLLPM_MED
        print LIST="TOLLPM_LRG: ",LI.1.TOLLPM_LRG," ", LI.2.TOLLPM_LRG

        print LIST="TOLLEV_DA : ",LI.1.TOLLEV_DA ," ", LI.2.TOLLEV_DA 
        print LIST="TOLLEV_S2 : ",LI.1.TOLLEV_S2 ," ", LI.2.TOLLEV_S2 
        print LIST="TOLLEV_S3 : ",LI.1.TOLLEV_S3 ," ", LI.2.TOLLEV_S3 
        print LIST="TOLLEV_VSM: ",LI.1.TOLLEV_VSM," ", LI.2.TOLLEV_VSM
        print LIST="TOLLEV_SML: ",LI.1.TOLLEV_SML," ", LI.2.TOLLEV_SML
        print LIST="TOLLEV_MED: ",LI.1.TOLLEV_MED," ", LI.2.TOLLEV_MED
        print LIST="TOLLEV_LRG: ",LI.1.TOLLEV_LRG," ", LI.2.TOLLEV_LRG
    ENDIF
  ENDPHASE

  PHASE=SUMMARY
    print LIST="TOTAL MISMATCH = ",_TOTALMISMATCH
  ENDPHASE
ENDRUN